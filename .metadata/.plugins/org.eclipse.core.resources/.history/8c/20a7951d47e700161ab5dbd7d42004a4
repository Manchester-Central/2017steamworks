
public class yes {

	
	public static void main(String[] args)
	{
		
		int sum = 0;
		int i = 3;
		
		for (i = 3; i < 1000; i ++ )
		{
			if ( i % 3 == 0 || i % 5 == 0)
			{
				sum += i;
			}
		}
		System.out.println(sum);
		
	}
	
	static void printPE_1()
	{
		int sum = 0;
		int i = 3;
		
		for (i = 3; i < 1000; i ++ )
		{
			if ( i % 3 == 0 || i % 5 == 0)
			{
				sum += i;
			}
		}
		System.out.println(sum);
	}
}
